# Simulate Parkes telescope observing Vela 
<define>
name: parkes_0.5d_sim
nproc: 1
nreal: 1
</define>

<pulsars>
psr: name=J0711-6830
psr: name=J2129-5721
</pulsars>

<obsRun>
name: PARKES 
tel: pks 
start: 53043.0
finish: 53043.5
sampling: cadence=0.042
sched: sched1
</obsRun>

<schedule>
name: sched1
observe: psr=J0711-6830,toaerr=1.45e-7,freq=1400,ha=8*(ran(linear)-0.5)
observe: psr=J2129-5721,toaerr=1.45e-7,freq=1400,ha=8*(ran(linear)-0.5)
</schedule>
