# Simulate Parkes telescope observing Vela 
<define>
name: parkes_5d_sim
nproc: 1
nreal: 1
</define>

<pulsars>
psr: name=J1744-1134
psr: name=J1545-4550
</pulsars>

<obsRun>
name: PARKES 
tel: pks 
start: 53043.0
finish: 53048.0
sampling: cadence=0.042
sched: sched1
</obsRun>

<schedule>
name: sched1
observe: psr=J1744-1134,toaerr=1.45e-7,freq=1400,ha=8*(ran(linear)-0.5)
observe: psr=J1545-4550,toaerr=1.45e-7,freq=1400,ha=8*(ran(linear)-0.5)
</schedule>
