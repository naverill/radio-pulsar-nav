# Simulate Parkes telescope observing Vela 
<define>
name: parkes_0.168d_sim
nproc: 1
nreal: 1
</define>

<pulsars>
psr: name=J1024-0719
psr: name=J1824-2452A
</pulsars>

<obsRun>
name: PARKES 
tel: pks 
start: 53043.0
finish: 53043.168
sampling: cadence=0.042
sched: sched1
</obsRun>

<schedule>
name: sched1
observe: psr=J1024-0719,toaerr=1.45e-7,freq=1400,ha=8*(ran(linear)-0.5)
observe: psr=J1824-2452A,toaerr=1.45e-7,freq=1400,ha=8*(ran(linear)-0.5)
</schedule>
